<?xml version='1.0' encoding='utf-8'?>
<widget id="com.divideandconquer.jucsemylib" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>JU-CSE-MyLibrary</name>
    <description>
        Jadavpur University CSE departmental library applicaton
    </description>
    <author email="android-project-ju@googlegroups.com" href="https://github.com/JuDivideAndConquer/JU-CSE-MyLibrary">
        JuDivideAndConquer
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/36.png" />
    <icon density="mdpi" src="res/icon/android/48.png" />
    <icon density="hdpi" src="res/icon/android/72.png" />
    <icon density="xhdpi" src="res/icon/android/96.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
